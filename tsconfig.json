{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            //"@/*": ["./*"],
            "@assets/*": ["assets/*"],
            "@fofnts/*": ["assets/fonts/*"],
            "@images/*": ["assets/images/*"],
            "@components/*": ["components/*"],
            "@constants/*": ["constants/*"],
            "@routes/*": ["routes/*"],
            "@hooks/*": ["hooks/*"],
            "@utils/*": ["utils/*"]
        }
    },
    "extends": "expo/tsconfig.base",
    "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
